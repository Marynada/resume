<div>
  <div>
    <h1 class='text-center'>{{page_header/title}}</h1>
    <p class='text-center text-uppercase'>
      {{page_header/description}}
    </p>
    <div class='card'>
      <div class='card-body'>
        <div class='display-6 text-end'>
          {{section/about_us/title_list/main}}
        </div>
        <h2 class='text-end'>
          {{section/about_us/title_list/sub}}
        </h2>
        <button class='btn btn-outline-info'>
          {{section/about_us/button/text}}
        </button>
      </div>
    </div>
    <br />
    <div class='card'>
      <div class='card-body'>
        <div class='display-5'>
          {{section/advantage/title}}
        </div>
        <hr />
        <div class='display-4'>
          {{section/advantage/efficiency/title}}
        </div>
        <h6>
          {{section/advantage/efficiency/sub_title}}
        </h6>
        <p>
          {{section/advantage/efficiency/description}}
        </p>
        <button class='btn btn-outline-warning'>
          {{section/advantage/efficiency/button}}
        </button>
        <hr />
        <div class='display-4 text-center'>
          {{section/advantage/innovation/title}}
        </div>
        <h6 class='text-center'>
          {{section/advantage/innovation/sub_title}}
        </h6>
        <p class='text-center'>
          {{section/advantage/innovation/description}}
        </p>
        <button class='btn btn-outline-warning'>
          {{section/advantage/innovation/button}}
        </button>
        <hr />
        <div class='display-4 text-end'>
          {{section/advantage/collaboration/title}}
        </div>
        <h6 class='text-end'>
          {{section/advantage/collaboration/sub_title}}
        </h6>
        <p class='text-end'>
          {{section/advantage/collaboration/description}}
        </p>
        <button class='btn btn-outline-warning'>
          {{section/advantage/collaboration/button}}
        </button>
        <hr />
      </div>
    </div>
    <br />
    <div class='card'>
      <div class='card-body'>
        <div class='display-5'>
          {{section/agreement/title}}
        </div>
        <hr />
        <h6>
          {{section/agreement/info/sub_title}}
        </h6>
        <div class='card'>
          <div class='card-body'>
            <p>
              {{section/agreement/info/detail/text}}
            </p>
            <button class='btn btn-success'>
              {{section/agreement/info/detail/button}}
            </button>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>